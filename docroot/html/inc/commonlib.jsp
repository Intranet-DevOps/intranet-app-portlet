<div id="commonConfirm" class="modal fade paraModal"
	style="bottom: auto; width: 600px;" role="dialog">
	<div class="modal-dialog paraModalDialog"
		style="margin-top: 0px; margin-bottom: 0px;">

		<!-- Modal content-->
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal">&times;</button>
				<h4 class="modal-title" id="commonConfirmTitle">Confirmation</h4>
			</div>
			<div class="modal-body">
				<p id="commonConfirmMessage">Confirmation</p>
			</div>
		</div>

	</div>
</div>

<!-- Modal -->
<div id="commonDialog" class="modal fade paraModal"
	style="bottom: auto; width: 600px;" role="dialog">
	<div class="modal-dialog paraModalDialog"
		style="margin-top: 0px; margin-bottom: 0px;">

		<!-- Modal content-->
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal">&times;</button>
				<h4 class="modal-title" id="commonDialogTitle"></h4>
			</div>
			<div class="modal-body">
				<p id="commonDialogMessage"></p>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default"
					id="commonDialogYesButton" data-dismiss="modal">Yes</button>
				<button type="button" class="btn btn-default"
					id="commonDialogNoButton" data-dismiss="modal">No</button>
			</div>
		</div>

	</div>
</div>

 
<!-- Modal -->    
<div class="modal commonParaModal" id="pleaseWaitDialog" style="height: 131px;"  
	data-keyboard="false">
	<div class="modal-header">
		<h4>Working on it</h4>
	</div>
	<div class="modal-body">
		
		<div class="progress progress-striped active">
			<div class="bar" style="width: 100%;"></div>
		</div>
	</div>
</div>